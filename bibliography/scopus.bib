@ARTICLE{Hayatdavoodi2021,
author={Hayatdavoodi, D.S. and Razavi, N. and Bidhendi, M.Q.},
title={Probing Through Iranian Architectural History Within the Framework of an Ontology Development Process},
journal={Digital Humanities Quarterly},
year={2021},
volume={15},
number={3},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85135308180&partnerID=40&md5=bebf0a876672de15c158dcb905ab60b0},
affiliation={Faculty of Architecture & Urban Planning, Shahid Beheshti University, Iran},
abstract={This paper presents a prototype ontology developed in the field of Iranian architectural history. The paper’s central arguments offer a response to questions regarding how to create an ontology in Iranian architectural history, what consideration must be addressed here, and how to resolve problematic issues in developing an ontology in a field such as Iranian architectural history, which lacks a formalized knowledge. The paper is organized into two parts. It primarily presents a discussion on the specific domain of architectural history and what it encompasses and moves on to examine why it is deemed complicated. After that, the process of creating Iranian architectural history ontology and the methodology applied to match the intended domain is explained. In the second part of the paper, the content of the developed ontology is discussed, which includes various parts of the ontology and what it implies to illustrate how the structure of ontology helps in logically representing the domain in a machine-readable format. © 2021, Alliance of Digital Humanities Organisations. All rights reserved.},
funding_text 1={This research has been developed within the master thesis Project of “Ontology of Iranian Architectural History: based on contemporary scholarly publications” in Shahid Beheshti University (2017) under the supervision of Dr. Niloofar Razavi and consultation of Dr. Mehrdad Qayyoomi Bidhendi. We would like to express our personal gratitude to all those who have helped in carrying out the research.},
