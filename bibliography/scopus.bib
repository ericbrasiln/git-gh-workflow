@CONFERENCE{Son2019,
author={Son, J. and Negahban, A. and Chiang, D.T.},
title={Topic diversity of online consumer reviews and its effect on review helpfulness},
journal={25th Americas Conference on Information Systems, AMCIS 2019},
year={2019},
note={cited By 3; Conference of 25th Americas Conference on Information Systems, AMCIS 2019 ; Conference Date: 15 August 2019 Through 17 August 2019;  Conference Code:151731},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85084021366&partnerID=40&md5=6f36e26cb7300de42cf7f67c819e7b13},
affiliation={California State University, Chico, United States},
abstract={Online consumer reviews can help potential buyers increase product diagnosticity and thus decrease product uncertainty. With the rapid growth of mobile and online shopping, understanding factors that influence the helpfulness of consumer reviews becomes crucial. Among other factors, review content as a main vehicle for conveying information has drawn great attention. In this study, we propose a new measure called topic diversity to estimate reviews’ information. By leveraging statistical and predictive analyses, we furnish empirical evidence to demonstrate a reliable relationship between our proposed measure and review helpfulness. Our findings suggest that topic diversity is another measure to estimate reviews’ conveying information along with the number of words. This study contributes to academia and industry, respectively. For academic researchers, the proposed measure enhances our understanding of consumer reviews’ features in association with helpfulness. Industry practitioners can use reviews’ topic diversity to recommend more helpful reviews to potential buyers. © 2019 Association for Information Systems. All rights reserved.},
author_keywords={Online consumer review;  Review helpfulness;  Review length;  Topic diversity;  Topic modeling},
keywords={Information systems;  Information use, Consumer reviews;  Online consumer reviews;  Online shopping;  Potential buyers;  Product uncertainties;  Review contents;  Topic diversity;  Topic Modeling, Electronic commerce},
