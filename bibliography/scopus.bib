@ARTICLE{Tangherlini2014223,
author={Tangherlini, T.R. and Broadwell, P.M.},
title={Sites of (re)collection: Creating the danish folklore nexus},
journal={Journal of Folklore Research},
year={2014},
volume={51},
number={2},
pages={223-247},
doi={10.1353/jfr.2014.0008},
note={cited By 6},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84908451380&doi=10.1353%2fjfr.2014.0008&partnerID=40&md5=23cc8cac4cbf4bce234f95017b91e8e1},
affiliation={Scandinavian Section, UCLA, United States; Library, UCLA, United States},
abstract={We explore the scholarly concerns and technical considerations involved in creating a digital interface to the folklore collections of Evald Tang Kristensen (1843-1929), one of the world’s most prolific collectors of folklore. The two interfaces we designed, the Danish Folklore Nexus (DFL 1.0) and ETKSpace (DFL 2.0), represent differing approaches to the challenges inherent in curating a large collection of non-digital materials for online presentation and designing a digital environment that enables folklore scholars to access and explore the collection in a straightforward and intellectually enlightening manner. Among these challenges are those related to the bulk digitization of archival assets, the storage of those assets in a secure and persistent manner, and the selection of appropriate presentation technologies, database structures, and metadata formats. Predicated on the idea of fusing “distant reading” (Moretti 2000) to “close reading,” the Danish Folklore Nexus provides one possible solution to the pressing need for simple yet robust methods for navigating a folklore collection along multiple axes of introspection. We also present our work on ETKSpace (DFL 2.0), an online portal to the entire corpus (approximately three thousand informants, twelve thousand place names, and sixty thousand records) that incorporates a faceted browsing interface similar to an online shopping site, allowing researchers to rapidly delimit and export research corpora for more detailed types of analysis. Our discussion presents “lessons learned” as well as considerations of possible future directions for this type of digital archival scholarship. © 2014 Department of Folklore and Ethnomusicology, Indiana University.},
references={Abello, J., Broadwell, P., Tangherlini, T.R., Computational Folkloristics (2012) Communications of the Association for Computing Machinery, 55 (7), pp. 60-70; (2006) Our Cultural Commonwealth: The Report of the ACLS Commission on Cyberinfrastructure for the Humanities and Social Sciences, , New York: American Council of Learned Societies; Börner, K., Plug-and-Play Macroscopes (2011) Communications of the ACM, 54 (3), pp. 60-69; Bricka, C.F., Laursen, L.R., Steenstrup, J., (1938) Dansk biografisk lexikon, 13. , 2nd ed, Copenhagen: J. H. Schultz; Broadwell, P.M., Tangherlini, T.R., TrollFinder: Geo-Semantic Exploration of a Very Large Corpus of Danish Folklore (2012) Proceedings of LREC: Workshop on Computational Models of Narrative, pp. 50-55. , http://www.lrec-conf.org/proceedings/lrec2012/index.html, edited by Mark A. Finlayson, Istanbul: Language Resources and Evaluation Conference; Christiansen, P.O., (2011) De forsvunde. Hedens sidste fortællere, , Copenhagen: Gads forlag; Christiansen, R., The Migratory Legends (1992) FF Communications, 175. , (1958), Helsinki: Suomalainen Tiedeakatemia; (2008) No Brief Candle: Reconceiving Research Libraries for the 21st Century, , Washington, DC: Council on Library and Information Resources; Dundes, A., (1980) The Morphology of North American Indian Folktales. FF Communications, p. 195. , (1964), Helsinki: Suomalainen Tiedeakatemia; Geertz, C., Thick Description: Toward an Interpretive Theory of Culture (1973) The Interpretation of Cultures: Selected Essays, pp. 3-30. , New York: Basic Books; Graham, S., Weingart, S., Milligan, I., Getting Started with Topic Modeling and MALLET (2012) The Programming Historian, , http://programminghistorian.org/lessons/topic-modeling-and-mallet, 2, edited by Adam Crymble, Fred Gibbs, and Miriam Posner; Gunnell, T., (2008) Legends and Landscape: Articles Based on Plenary Papers Presented at the 5th Celtic-Nordic-Baltic Folklore Symposium, , Reykjavík, Reykjavik: University of Icela