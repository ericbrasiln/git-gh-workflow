@BOOK{Tello20211,
author={Tello, J.C.},
title={The Novel in the Spanish Silver Age A Digital Analysis of Genre Using Machine Learning},
journal={The Novel in the Spanish Silver Age: A Digital Analysis of Genre Using Machine Learning},
year={2021},
pages={1-468},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85124858169&partnerID=40&md5=f57909e3688cfb75566752c0027b8300},
