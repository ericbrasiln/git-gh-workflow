@ARTICLE{Milligan2020235,
author={Milligan, I.},
title={History in the era of abundance: Web archives and historical research [L'histoire à l'ère de l'abondance: Archives web et recherche historique] [La historia en la era de la abundancia: Archivos web e investigación histórica]},
journal={Historia y Memoria},
year={2020},
pages={235-269},
doi={10.19053/20275137.NESPECIAL.2020.11587},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85092635271&doi=10.19053%2f20275137.NESPECIAL.2020.11587&partnerID=40&md5=6677c6629d702a5b48e9e91974a37963},
affiliation={Universidad de York, United Kingdom; University of Waterloo, Canada},
abstract={Can the 1990s be considered the history of the present? As historians turn to study this period and beyond, they will encounter a historical record that is radically different from what has ever existed before. Old websites, social media, blogs, photographs, and videos are all part of the massive quantities of digital information that technologists, librarians, archivists, and organizations such as the Internet Archive have been collecting for the past three decades. This article explores how this dramatic shift in our historical record changes the work of historians. It does so in two main ways. First, it outlines the approaches, methods, tools, and search functions that can help a historian turn web documents into historical sources. Secondly, it considers the implications of the size and scale of digital sources, which amount to more information than historians have ever had at their fingertips, and much of which are by and about people who have traditionally been absent from the historical record. As a way to make these points tangible, it does so primarily through an in-depth case study of working with the GeoCities. com web archive, a collection of hundreds of millions of 1990s webpages. © 2020 Universidad Pedagogica y Tecnologica de Colombia, Instituto de Investigaciones y Formacion Avanzada. All rights reserved.},
author_keywords={Digital history;  Digital humanities;  Historiography;  Web archives},
